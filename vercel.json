{
  "version": 2,
  "functions": {
    "api/todos.js": {
      "runtime": "@vercel/node@latest",
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/todos",
      "dest": "/api/todos",
      "methods": ["GET"]
    },
    {
      "src": "/",
      "dest": "/api/todos",
      "methods": ["GET"]
    }
  ]
}
